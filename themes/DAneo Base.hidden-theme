		"quick_panel_fg-hover-match-light": "color(var(accent) blend(var(label_fg-hover-light) 75%)",

		"quick_panel_fg-selected-dark": "var(label_fg-selected-dark)",
		"quick_panel_fg-selected-light": "var(label_fg-selected-light)",
		"quick_panel_fg-selected-match-dark": "color(var(accent) blend(var(quick_panel_fg-selected-dark) 90%)",
		"quick_panel_fg-selected-match-light": "color(var(accent) blend(var(quick_panel_fg-selected-light) 90%)",

		"quick_panel_fg-selected-hover-dark": "var(label_fg-selected-hover-dark)",
		"quick_panel_fg-selected-hover-light": "var(label_fg-selected-hover-light)",
		"quick_panel_fg-selected-hover-match-dark": "color(var(accent) blend(var(quick_panel_fg-selected-hover-dark) 90%)",
		"quick_panel_fg-selected-hover-match-light": "color(var(accent) blend(var(quick_panel_fg-selected-hover-light) 90%)",

		// Quick Panel Description Text

		"quick_panel_path_font_face": "var(font_face)",
		"quick_panel_path_font_size": 10,
		"quick_panel_path_font_italic": true,

		// Side Bar //////////////////////////////////////////////////////////////

		// Background Colors

		"sidebar_bg-normal-dark": "var(overlay_bg-dark)",
		"sidebar_bg-normal-light": "var(overlay_bg-light)",
		"sidebar_bg-hover-dark": "var(item_bg-hover-dark)",
		"sidebar_bg-hover-light": "var(item_bg-hover-light)",
		"sidebar_bg-selected-dark": "var(item_bg-selected-dark)",
		"sidebar_bg-selected-light": "var(item_bg-selected-light)",
		"sidebar_bg-selected-hover-dark": "var(item_bg-selected-hover-dark)",
		"sidebar_bg-selected-hover-light": "var(item_bg-selected-hover-light)",

		// Headings Text

		"sidebar_heading_font_face": "var(font_face)",
		"sidebar_heading_font_size": "var(font_size)",
		"sidebar_heading_font_bold": false,
		"sidebar_heading_font_italic": false,

		"sidebar_heading_fg-dark": "rgba(255, 255, 255, 0.2)",
		"sidebar_heading_fg-light": "rgba(0, 0, 0, 0.2)",

		// Tree Items Text

		"sidebar_font_face": "var(font_face)",
		"sidebar_font_size": "var(font_size)",

		"sidebar_fg-normal-dark": "var(label_fg-normal-dark)",
		"sidebar_fg-normal-light": "var(label_fg-normal-light)",
		"sidebar_fg-hover-dark": "var(label_fg-hover-dark)",
		"sidebar_fg-hover-light": "var(label_fg-hover-light)",
		"sidebar_fg-selected-dark": "var(label_fg-selected-dark)",
		"sidebar_fg-selected-light": "var(label_fg-selected-light)",
		"sidebar_fg-selected-hover-dark": "var(label_fg-selected-hover-dark)",
		"sidebar_fg-selected-hover-light": "var(label_fg-selected-hover-light)",

		// Folder Icon Colors

		"sidebar_folder_icon_tint-collapsed-dark": "rgba(255, 255, 255, 0.6)",
		"sidebar_folder_icon_tint-collapsed-light": "rgba(0, 0, 0, 0.6)",
		"sidebar_folder_icon_tint-expanded-dark": "rgba(255, 255, 255, 0.6)",
		"sidebar_folder_icon_tint-expanded-light": "rgba(0, 0, 0, 0.6)",
		"sidebar_folder_icon_overlay-dark": "rgba(255, 255, 255, 0.3)",
		"sidebar_folder_icon_overlay-light": "rgba(0, 0, 0, 0.3)",

		// Icons Opacity

		"sidebar_icon_opacity-hidden": {
				"target": 0.00, "speed": 2.50, "interpolation": "smoothstep"
			},
		"sidebar_icon_opacity-normal": {
				"target": 0.75, "speed": 1.12, "interpolation": "smoothstep"
			},
		"sidebar_icon_opacity-hover": {
				"target": 0.90, "speed": 1.12, "interpolation": "smoothstep"
			},
		"sidebar_icon_opacity-selected": {
				"target": 0.85, "speed": 1.12, "interpolation": "smoothstep"
			},
		"sidebar_icon_opacity-selected-hover": {
				"target": 1.00, "speed": 1.12, "interpolation": "smoothstep"
			},

		// Disclosure Buttons

		"disclosure_fg-collapsed-normal-dark": "color(white l(- 22%))",
		"disclosure_fg-collapsed-normal-light": "color(black l(+ 22%))",
		"disclosure_fg-collapsed-hover-dark": "var(accent)",
		"disclosure_fg-collapsed-hover-light": "var(accent)",
		"disclosure_fg-collapsed-selected-dark": "color(white l(- 8%))",
		"disclosure_fg-collapsed-selected-light": "color(black l(+ 8%))",
		"disclosure_fg-collapsed-selected-hover-dark": "var(accent)",
		"disclosure_fg-collapsed-selected-hover-light": "var(accent)",

		"disclosure_fg-expanded-normal-dark": "color(white l(- 15%))",
		"disclosure_fg-expanded-normal-light": "color(black l(+ 15%))",
		"disclosure_fg-expanded-hover-dark": "var(accent)",
		"disclosure_fg-expanded-hover-light": "var(accent)",
		"disclosure_fg-expanded-selected-dark": "color(white l(- 5%))",
		"disclosure_fg-expanded-selected-light": "color(black l(+ 5%))",
		"disclosure_fg-expanded-selected-hover-dark": "var(accent)",
		"disclosure_fg-expanded-selected-hover-light": "var(accent)",

		// Tabs //////////////////////////////////////////////////////////////////

		"tab_font_face": "var(font_face)",
		"tab_font_size": "var(font_size)",

		"tabset_bg-dark": "var(bg)",
		"tabset_bg-light": "var(bg)",

		// Active Tab

		"active_tab_fg-normal-dark": "var(label_fg-selected-dark)",
		"active_tab_fg-normal-light": "var(label_fg-selected-light)",
		"active_tab_fg-hover-dark": "var(label_fg-selected-hover-dark)",
		"active_tab_fg-hover-light": "var(label_fg-selected-hover-light)",

		// close button of saved unhovered active tab
		"active_tab_close_opacity-clean": {
				"target": 0.70, "speed": 2.50, "interpolation": "smoothstep"
			},
		// close button of dirty unhovered active tab
		"active_tab_close_opacity-dirty": {
				"target": 0.70, "speed": 2.50, "interpolation": "smoothstep"
			},
		// close button of hovered active tab
		"active_tab_close_opacity-normal": {
				"target": 0.70, "speed": 1.12, "interpolation": "smoothstep"
			},
		// hovered close button on active tab
		"active_tab_close_opacity-hover": {
				"target": 1.00, "speed": 1.12, "interpolation": "smoothstep"
			},

		// Inactive Tab

		"inactive_tab_bg-normal-dark": "var(tabset_bg-dark)",
		"inactive_tab_bg-normal-light": "var(tabset_bg-light)",
		"inactive_tab_bg-hover-dark": "var(tabset_bg-dark)",
		"inactive_tab_bg-hover-light": "var(tabset_bg-light)",

		"inactive_tab_fg-normal-dark": "var(label_fg-normal-dark)",
		"inactive_tab_fg-normal-light": "var(label_fg-normal-light)",
		"inactive_tab_fg-hover-dark": "var(label_fg-hover-dark)",
		"inactive_tab_fg-hover-light": "var(label_fg-hover-light)",

		// close button of saved unhovered inactive tab
		"inactive_tab_close_opacity-clean": {
				"target": 0.00, "speed": 2.50, "interpolation": "smoothstep"
			},
		// close button of dirty unhovered inactive tab
		"inactive_tab_close_opacity-dirty": {
				"target": 0.45, "speed": 2.50, "interpolation": "smoothstep"
			},
		// close button of hovered inactive tab
		"inactive_tab_close_opacity-normal": {
				"target": 0.45, "speed": 1.12, "interpolation": "smoothstep"
			},
		// hovered close button on inactive tab
		"inactive_tab_close_opacity-hover": {
				"target": 0.70, "speed": 1.12, "interpolation": "smoothstep"
			},

		// Grid Layout ///////////////////////////////////////////////////////////

		"grid_border_size": 2,

		"grid_border-dark": "var(tabset_bg-dark)",
		"grid_border-light": "var(tabset_bg-light)",

		// Fold Buttons //////////////////////////////////////////////////////////

		"fold_fg-collapsed-normal-dark": "var(button_icon_tint-normal-dark)",
		"fold_fg-collapsed-normal-light": "var(button_icon_tint-normal-light)",
		"fold_fg-collapsed-hover-dark": "var(button_icon_tint-hover-dark)",
		"fold_fg-collapsed-hover-light": "var(button_icon_tint-hover-light)",

		"fold_fg-expanded-normal-dark": "var(button_icon_tint-normal-dark)",
		"fold_fg-expanded-normal-light": "var(button_icon_tint-normal-light)",
		"fold_fg-expanded-hover-dark": "var(button_icon_tint-hover-dark)",
		"fold_fg-expanded-hover-light": "var(button_icon_tint-hover-light)",

		"fold_opacity-normal": "var(button_icon_opacity-normal)",
		"fold_opacity-hover": "var(button_icon_opacity-hover)",

		// Auto Completion ///////////////////////////////////////////////////////

		// Background Colors

		"auto_complete_bg-dark": "var(overlay_bg-dark)",
		"auto_complete_bg-light": "var(overlay_bg-light)",
		"auto_complete_bg-hover-dark": "var(item_bg-hover-dark)",
		"auto_complete_bg-hover-light": "var(item_bg-hover-light)",

		"auto_complete_bg-selected-dark": "var(item_bg-selected-dark)",
		"auto_complete_bg-selected-light": "var(item_bg-selected-light)",
		"auto_complete_bg-selected-hover-dark": "var(item_bg-selected-hover-dark)",
		"auto_complete_bg-selected-hover-light": "var(item_bg-selected-hover-light)",

		// Text Colors

		"auto_complete_fg-normal-dark": "var(label_fg-normal-dark)",
		"auto_complete_fg-normal-light": "var(label_fg-normal-light)",
		"auto_complete_fg-normal-match-dark": "color(var(accent) blend(var(auto_complete_fg-normal-dark) 55%)",
		"auto_complete_fg-normal-match-light": "color(var(accent) blend(var(auto_complete_fg-normal-light) 55%)",

		"auto_complete_fg-hover-dark": "var(label_fg-hover-dark)",
		"auto_complete_fg-hover-light": "var(label_fg-hover-light)",
		"auto_complete_fg-hover-match-dark": "color(var(accent) blend(var(auto_complete_fg-hover-dark) 75%)",
		"auto_complete_fg-hover-match-light": "color(var(accent) blend(var(auto_complete_fg-hover-light) 75%)",

		"auto_complete_fg-selected-dark": "var(label_fg-selected-dark)",
		"auto_complete_fg-selected-light": "var(label_fg-selected-light)",
		"auto_complete_fg-selected-match-dark": "color(var(accent) blend(var(auto_complete_fg-selected-dark) 90%)",
		"auto_complete_fg-selected-match-light": "color(var(accent) blend(var(auto_complete_fg-selected-light) 90%)",

		"auto_complete_fg-selected-hover-dark": "var(label_fg-selected-hover-dark)",
		"auto_complete_fg-selected-hover-light": "var(label_fg-selected-hover-light)",
		"auto_complete_fg-selected-hover-match-dark": "color(var(accent) blend(var(auto_complete_fg-selected-hover-dark) 90%)",
		"auto_complete_fg-selected-hover-match-light": "color(var(accent) blend(var(auto_complete_fg-selected-hover-light) 90%)",

		// Tooltips //////////////////////////////////////////////////////////////

		"tooltip_font_face": "var(font_face)",
		"tooltip_font_size": "var(font_size)",

		"tooltip_bg": "color(white blend(var(--yellowish) 5%))",
		"tooltip_bc": "color(white blend(var(--yellowish) 15%))",
		"tooltip_fg": "color(black blend(var(--yellowish) 66%))",

		// Panel Control /////////////////////////////////////////////////////////

		"panel_bg-dark": "color(black blend(var(bg) 20%))",
		"panel_bg-light": "color(black blend(var(bg) 10%))",

		// Status Bar ////////////////////////////////////////////////////////////

		"statusbar_font_face": "var(font_face)",
		"statusbar_font_size": "var(font_size)",

		"statusbar_bg-dark": "color(black blend(var(bg) 20%))",
		"statusbar_bg-light": "color(black blend(var(bg) 10%))",

		"statusbar_label_fg-normal-dark": "var(label_fg-normal-dark)",
		"statusbar_label_fg-normal-light": "var(label_fg-normal-light)",

		"statusbar_button_fg-normal-dark": "var(button_fg-normal-dark)",
		"statusbar_button_fg-normal-light": "var(button_fg-normal-light)",
		"statusbar_button_fg-hover-dark": "var(button_fg-hover-dark)",
		"statusbar_button_fg-hover-light": "var(button_fg-hover-light)",

		// VCS annotation

		"statusbar_vcs_font_face": "var(statusbar_font_face)",
		"statusbar_vcs_font_size": 10,

		"statusbar_vcs_fg-normal-dark": "color(var(accent) alpha(70%))",
		"statusbar_vcs_fg-normal-light": "color(var(accent) alpha(85%))",
		"statusbar_vcs_fg-hover-dark": "var(accent)",
		"statusbar_vcs_fg-hover-light": "var(accent)",

		"statusbar_vcs_bc-dark": "color(white alpha(10%))",
		"statusbar_vcs_bc-light": "color(black alpha(30%))",
		"statusbar_vcs_bc-hover-dark": "color(var(statusbar_vcs_bc-dark) alpha(+ 40%))",
		"statusbar_vcs_bc-hover-light": "color(var(statusbar_vcs_bc-light) alpha(+ 40%))",

		// Dialogs ///////////////////////////////////////////////////////////////

		"dialog_bg-dark":  "color(black blend(var(bg) 20%))",
		"dialog_bg-light": "color(black blend(var(bg) 10%))",

		"dialog_title_fg-dark": "color(white blend(var(dialog_bg-dark) 80%))",
		"dialog_title_fg-light": "color(black blend(var(dialog_bg-light) 80%))",

		"dialog_text_fg-dark": "color(white blend(var(dialog_bg-dark) 45%))",
		"dialog_text_fg-light": "color(black blend(var(dialog_bg-light) 45%))",

