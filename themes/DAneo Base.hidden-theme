		"quick_panel_fg-normal-match-light": "color(var(accent) blend(var(quick_panel_fg-normal-light) 55%)",

		"quick_panel_fg-hover-dark": "var(label_fg-hover-dark)",
		"quick_panel_fg-hover-light": "var(label_fg-hover-light)",
