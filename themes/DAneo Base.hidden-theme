{
	"variables": {

		// Color Scheme Alias

		"bg": "var(--background)",
		"fg": "var(--foreground)",

		"accent": "var(--accent)",

		"vsc_ignored": "color(var(fg) alpha(0.5))",
		"vsc_modified": "var(--orangish)",
		"vsc_staged": "var(--yellowish)",
		"vsc_unmerged": "var(--redish)",
		"vsc_deleted": "var(--redish)",
		"vsc_untracked": "grey",
		"vsc_added": "var(--greenish)",

		// MacOS Title Bar ///////////////////////////////////////////////////////

		"title_bg-dark": "color(black blend(var(bg) 7.5%))",
		"title_bg-light": "color(black blend(var(bg) 10%))",

		"title_fg-dark": "color(white blend(var(bg) 65%))",
		"title_fg-light": "color(black blend(var(bg) 65%))",

		// Label Controls ////////////////////////////////////////////////////////

		"font_face": "system",
		"font_size": 12,

		"label_fg-normal-dark":          "rgba(255, 255, 255, 0.45)",
		"label_fg-normal-light":         "rgba(000, 000, 000, 0.45)",
		"label_fg-hover-dark":           "rgba(255, 255, 255, 0.90)",
		"label_fg-hover-light":          "rgba(000, 000, 000, 0.90)",
		"label_fg-selected-dark":        "rgba(255, 255, 255, 0.80)",
		"label_fg-selected-light":       "rgba(000, 000, 000, 0.80)",
		"label_fg-selected-hover-dark":  "rgba(255, 255, 255, 0.95)",
		"label_fg-selected-hover-light": "rgba(000, 000, 000, 0.95)",

		// List & Tree Items /////////////////////////////////////////////////////

		"item_bg-dark": "rgba(255, 255, 255, 0.05)",
		"item_bg-light": "rgba(0, 0, 0, 0.075)",
		"item_bg-hover-dark": "var(item_bg-dark)",
		"item_bg-hover-light": "var(item_bg-light)",
		"item_bg-selected-dark": "var(item_bg-dark)",
		"item_bg-selected-light": "var(item_bg-light)",
		"item_bg-selected-hover-dark": "var(item_bg-dark)",
		"item_bg-selected-hover-light": "var(item_bg-light)",

		"item_bg-opacity-normal": {
				"target": 0.0, "speed": 7.0, "interpolation": "linear"
			},
		"item_bg-opacity-hover": {
				"target": 0.0, "speed": 7.0, "interpolation": "smoothstep"
			},
		"item_bg-opacity-selected": {
				"target": 0.9, "speed": 7.0, "interpolation": "smoothstep"
			},
		"item_bg-opacity-selected-hover": {
				"target": 1.0, "speed": 7.0, "interpolation": "smoothstep"
			},

		// Button Controls ///////////////////////////////////////////////////////

		"button_font_face": "var(font_face)",
		"button_font_size": "var(font_size)",

		"button_fg-normal-dark": "var(label_fg-normal-dark)",
		"button_fg-normal-light": "var(label_fg-normal-light)",
		"button_fg-hover-dark": "var(label_fg-hover-dark)",
		"button_fg-hover-light": "var(label_fg-hover-light)",
		"button_fg-selected-dark": "var(label_fg-selected-dark)",
		"button_fg-selected-light": "var(label_fg-selected-light)",

		"button_icon_tint-normal-dark": "rgba(255, 255, 255, 0.7)",
		"button_icon_tint-normal-light": "rgba(0, 0, 0, 0.7)",
		"button_icon_tint-hover-dark": "rgba(255, 255, 255, 0.8)",
		"button_icon_tint-hover-light": "rgba(0, 0, 0, 0.9)",
		"button_icon_tint-selected-dark": "rgba(255, 255, 255, 0.9)",
		"button_icon_tint-selected-light": "rgba(0, 0, 0, 0.9)",
		"button_icon_tint-dirty-dark": "var(accent)",
		"button_icon_tint-dirty-light": "var(accent)",

		"button_icon_opacity-normal": {
				"target": 0.45, "speed": 1.12, "interpolation": "smoothstep"
			},
		"button_icon_opacity-hover": {
				"target": 0.70, "speed": 1.12, "interpolation": "smoothstep"
			},
		"button_icon_opacity-selected": {
				"target": 1.00, "speed": 1.12, "interpolation": "smoothstep"
			},

		// Overlay Controls //////////////////////////////////////////////////////

		"overlay_bg-dark": "color(black blend(var(bg) 15%))",
		"overlay_bg-light": "color(black blend(var(bg) 7.5%))",

		// Quick Panels Background Colors

		"quick_panel_bg-dark": "var(overlay_bg-dark)",
		"quick_panel_bg-light": "var(overlay_bg-light)",
		"quick_panel_bg-hover-dark": "var(item_bg-hover-dark)",
		"quick_panel_bg-hover-light": "var(item_bg-hover-light)",

		"quick_panel_bg-selected-dark": "var(item_bg-selected-dark)",
		"quick_panel_bg-selected-light": "var(item_bg-selected-light)",
		"quick_panel_bg-selected-hover-dark": "var(item_bg-selected-hover-dark)",
		"quick_panel_bg-selected-hover-light": "var(item_bg-selected-hover-light)",

		// Quick Panel Caption Text

		"quick_panel_text_font_face": "var(font_face)",
		"quick_panel_text_font_size": "var(font_size)",

		"quick_panel_fg-normal-dark": "var(label_fg-normal-dark)",
		"quick_panel_fg-normal-light": "var(label_fg-normal-light)",
