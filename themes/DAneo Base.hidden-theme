	"rules": [

		// Title Bar (MacOS) /////////////////////////////////////////////////////

		{
			"class": "title_bar",
			"bg": "var(title_bg-dark)",
			"fg": "var(title_fg-dark)"
		},
		{
			"class": "title_bar",
			"attributes": ["file_light"],
			"bg": "var(title_bg-light)",
			"fg": "var(title_fg-light)"
		},

		// Labels ////////////////////////////////////////////////////////////////

		{
			"class": "label_control",
			"fg": "var(label_fg-normal-dark)",
			"font.face": "var(font_face)",
			"font.size": "var(font_size)"
		},
		{
			"class": "label_control",
			"parents": [
				{"class": "window", "attributes": ["file_light"]}
			],
			"fg": "var(label_fg-normal-light)"
		},

		// Text Line Control /////////////////////////////////////////////////////

		{
			"class": "text_line_control",
			"content_margin": [8, 6]
		},

		// Button Control ////////////////////////////////////////////////////////

		// Button (Normal)

		{
			"class": "button_control",
			"content_margin": 8,
			"min_size": [80, 0],
			"layer0.inner_margin": 6,
			"layer0.texture": "Theme - DAneo/textures/inputs/input--bw0--br2.png",
			"layer0.tint": "rgba(255, 255, 255, 0.1)",
			"layer0.opacity": {
				"target": 0.5, "speed": 2.25, "interpolation": "smoothstep"
			}
		},
		{
			"class": "button_control",
			"platforms": ["osx", "linux"],
			"content_margin": 9
		},
		{
			"class": "button_control",
			"parents": [
				{"class": "window", "attributes": ["file_light"]}
			],
			"layer0.tint": "rgba(0, 0, 0, 0.1)"
		},

		// Button (Hover)

		{
			"class": "button_control",
			"attributes": ["hover"],
			"layer0.opacity": {
				"target": 1.0, "speed": 2.25, "interpolation": "smoothstep"
			}
		},

		// Button (Pressed)

		{
			"class": "button_control",
			"attributes": ["pressed"],
			"layer0.opacity": {
				"target": 0.75, "speed": 4.5, "interpolation": "smoothstep"
			}
		},

		// Button (Normal) -> Label

		{
			"class": "label_control",
			"parents": [
				{"class": "button_control"}
			],
			"fg": "var(button_fg-normal-dark)",
			"font.face": "var(button_font_face)",
			"font.size": "var(button_font_size)"
		},
		{
			"class": "label_control",
			"parents": [
				{"class": "window", "attributes": ["file_light"]},
				{"class": "button_control"}
			],
			"fg": "var(button_fg-normal-light)"
		},

		// Button (Hover) -> Label

		{
			"class": "label_control",
			"parents": [
				{"class": "button_control", "attributes": ["hover"]}
			],
			"fg": "var(button_fg-hover-dark)"
		},
		{
			"class": "label_control",
			"parents": [
				{"class": "window", "attributes": ["file_light"]},
				{"class": "button_control", "attributes": ["hover"]}
			],
